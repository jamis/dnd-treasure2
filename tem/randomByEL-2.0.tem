            <OPTION VALUE="20" <!--%LEVEL:20%-->>20</OPTION>
            <OPTION VALUE="21" <!--%LEVEL:21%-->>21</OPTION>
            <OPTION VALUE="22" <!--%LEVEL:22%-->>22</OPTION>
            <OPTION VALUE="23" <!--%LEVEL:23%-->>23</OPTION>
            <OPTION VALUE="24" <!--%LEVEL:24%-->>24</OPTION>
            <OPTION VALUE="25" <!--%LEVEL:25%-->>25</OPTION>
            <OPTION VALUE="26" <!--%LEVEL:26%-->>26</OPTION>
            <OPTION VALUE="27" <!--%LEVEL:27%-->>27</OPTION>
            <OPTION VALUE="28" <!--%LEVEL:28%-->>28</OPTION>
            <OPTION VALUE="29" <!--%LEVEL:29%-->>29</OPTION>
            <OPTION VALUE="30" <!--%LEVEL:30%-->>30</OPTION>
          </SELECT>
          <INPUT TYPE="SUBMIT" VALUE="Generate Treasure"><BR>
					<TABLE BORDER=0><TR>
						<TD CLASS="NORMAL"><A HREF="javascript:explainCoins()">Coins</A>:</TD>
